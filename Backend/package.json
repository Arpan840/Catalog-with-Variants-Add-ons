{
 "scripts": {
  "dev": "nodemon --watch ./ --ext ts --exec ts-node server.ts",
  "start": "node dist/server.js",
  "build": "tsc",
  "serve": "ts-node server.ts",
  "migration:generate": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate ./src/migrations/InitSchema -d ./src/dataSource.ts",
  "migration:run": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/dataSource.ts",
  "migration:revert": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d ./src/dataSource.ts",
  "generate-module": "nodemon --watch src --ext ts --exec tsx src/scripts/generate-module.ts"
 },
 "devDependencies": {
  "@types/cors": "^2.8.19",
  "@types/express": "^5.0.3",
  "@types/node": "^24.0.1",
  "dotenv": "^16.6.1",
  "eslint": "^9.29.0",
  "express": "^5.1.0",
  "nodemon": "^3.1.10",
  "prettier": "^3.5.3",
  "ts-node": "^10.9.2",
  "tsconfig-paths": "^4.2.0",
  "tsx": "^4.20.3",
  "typescript": "^5.8.3"
 },
 "dependencies": {
  "cors": "^2.8.5",
  "pg": "^8.16.3",
  "reflect-metadata": "^0.2.2",
  "typeorm": "^0.3.25"
 }
}
